<center>
    <h1>New Item Credentials for <%= @merchant.name %></h1>
    <%= form_for @item, :url => {:action => "create"} do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :description %>
      <%= f.text_field :description %>
      <%= f.label :price %>
      <%= f.text_field :price %>
      <%= f.label :image %>
      <%= f.text_field :image %>
      <%= f.label :inventory %>
      <%= f.text_field :inventory %>
      <%= f.submit 'Submit' %>
    <% end %>
</center>